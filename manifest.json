{
    "manifest_version": 3,
    "name": "Disable Third-Party Cookies",
    "description": "Automatically disables third-party cookies in Chrome.",
    "version": "1.0",
    "permissions": ["privacy", "storage"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "icons": {
      "48": "icon.png"
    }
  }
  
